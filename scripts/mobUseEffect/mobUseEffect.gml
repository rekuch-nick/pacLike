function mobUseEffect(e){
	
	if(e == imgFire){
		
		ww.fmaps[array_length(ww.fmaps)] = {
			img: e,
			a: xSpot - 3,
			b: ySpot - 3,
			w: 7,
			h: 7,
			cd: 150 + (20 * pc.stage),
		}
	
	
	}
	
	
	if(e == imgWeb){
		
		ww.fmaps[array_length(ww.fmaps)] = {
			img: e,
			a: xSpot - 5,
			b: ySpot - 5,
			w: 11,
			h: 11,
			cd: 220 + (20 * pc.stage),
		}
	
	
	}
	
	
	
	
}